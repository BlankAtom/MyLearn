<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

    <!--数据源交给spring做-->

    <settings>
        <setting name="logImpl" value="STDOUT_LOGGING"/>
    </settings>

    <typeAliases>
        <package name="jmu.Hong.pojo"/>
    </typeAliases>

    <mappers>
        <mapper class="jmu.Hong.mapper.LoginMapper"/>
        <mapper class="jmu.Hong.mapper.StudentMapper"/>
        <mapper class="jmu.Hong.mapper.TeacherMapper"/>
        <mapper class="jmu.Hong.mapper.AdminMapper"/>
    </mappers>
    
</configuration>